			StarsAboveServersideConfig: {
				DisplayName: Stars Above Config (Server-side)

				DisableTypePenalty: {
					Label: Disable damage penalty for Aspected Damage modification
					Tooltip: 10% damage penalty when Aspected Damage modification is enabled is removed. Note that some effects that also remove this effect will be overpowered as a result.
				}

				DisableBossAggro: {
					Label: Disable Boss Aggro Marker
					Tooltip: Disables the marker that appears over a player's head when a boss targets them. This is a WIP feature.
				}

				DisableCalamityWeaponBuffs: {
